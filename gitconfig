# This is Git's per-user configuration file.
[include]
  path = .gitconfig-personal

[core]
  editor = nvim

[diff]
  tool = vscode

[difftool "vscode"]
  cmd = code --wait --diff $LOCAL $REMOTE

[merge]
  tool = vscode

[mergetool "vscode"]
  cmd = code --wait $MERGED

[push]
	autoSetupRemote = true

